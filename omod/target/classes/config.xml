<?xml version="1.0" encoding="UTF-8"?>

<module configVersion="1.2">

	<!-- Module Properties -->
	<id>maternityapp</id>
	<name>Maternity App Module Module</name>
	<version>2.0.1-SNAPSHOT</version>
	<package>org.openmrs.module.maternityapp</package>
	<author>bespoke updated by HealthIT</author>
	<description>
		Openmrs module to help trach maternity visits
	</description>
	<!--<updateURL>https://modules.openmrs.org/modules/download/maternityapp/update.rdf</updateURL>-->
	<require_version>2.3.3</require_version>
	<!-- / Module Properties -->

	
	<!-- Required Modules -->
	<require_modules>
        <require_module>org.openmrs.module.hospitalcore</require_module>
		<require_module>org.openmrs.module.uiframework</require_module>
        <require_module>org.openmrs.module.uicommons</require_module>
        <require_module>org.openmrs.module.mchapp</require_module>
		<require_module>org.openmrs.module.metadatadeploy</require_module>
    </require_modules>
	<!-- / Required Modules -->
	
	<!-- Module Activator -->
	<activator>org.openmrs.module.maternityapp.MaternityAppModuleActivator</activator>
	<aware_of_modules>
		<aware_of_module>org.openmrs.module.legacyui</aware_of_module>
	</aware_of_modules>
	
	<!-- Maps hibernate file's, if present -->
	<mappingFiles>
				
	</mappingFiles>

	<!-- Internationalization -->
	<!-- All message codes should start with maternityapp. -->
	<messages>
		<lang>en</lang>
		<file>messages.properties</file>
	</messages>
	<messages>
		<lang>fr</lang>
		<file>messages_fr.properties</file>
	</messages>
	<messages>
		<lang>es</lang>
		<file>messages_es.properties</file>
	</messages>
	<!-- /Internationalization -->

</module>

